<?xml version="1.0"?>
<layout version="1.0.0">
	<adminhtml_retaillog_index>
		<reference name="content">
			<block type="retailexpress/adminhtml_log" name="log" />
		</reference>
		<reference name="head">
			<action method="addJs">
				<script>retailexpress/retailexpress.js</script>
			</action>
		</reference>
	</adminhtml_retaillog_index>

	<adminhtml_retaillog_viewsynclogxml>
		<remove name="footer" />

		<reference name="content">
			<block type="retailexpress/adminhtml_viewsynclog" name="viewsynclogxml" />
		</reference>
		<reference name="head">
			<action method="addJs">
				<script>retailexpress/retailexpress.js</script>
			</action>
		</reference>
	</adminhtml_retaillog_viewsynclogxml>
</layout>
